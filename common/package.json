{
  "name": "dancedeets-common",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@babel/plugin-transform-flow-strip-types": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "lodash": "^4.17.2",
    "moment": "^2.17.0",
    "react": "^18.0.0",
    "react-intl": "^7.1.14",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "parse-json": "^2.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "underscore": "^1.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/js"],
    "testMatch": ["**/__tests__/**/*.test.ts", "**/__tests__/**/*.test.tsx"],
    "moduleNameMapper": {
      "dancedeets-common/(.*)": "<rootDir>/js/$1"
    },
    "transform": {
      "^.+\\.tsx?$": ["ts-jest", {
        "tsconfig": "<rootDir>/tsconfig.json"
      }]
    }
  }
}
