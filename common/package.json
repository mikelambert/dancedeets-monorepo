{
  "name": "dancedeets-common",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/js/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/js/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/js/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./js/*": {
      "import": {
        "types": "./dist/js/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "require": {
        "types": "./dist/js/*.d.ts",
        "default": "./dist/cjs/*.cjs"
      }
    },
    "./js/util/*": {
      "import": {
        "types": "./dist/js/util/*.d.ts",
        "default": "./dist/esm/util/*.js"
      },
      "require": {
        "types": "./dist/js/util/*.d.ts",
        "default": "./dist/cjs/util/*.cjs"
      }
    },
    "./js/events/*": {
      "import": {
        "types": "./dist/js/events/*.d.ts",
        "default": "./dist/esm/events/*.js"
      },
      "require": {
        "types": "./dist/js/events/*.d.ts",
        "default": "./dist/cjs/events/*.cjs"
      }
    },
    "./js/tutorials/*": {
      "import": {
        "types": "./dist/js/tutorials/*.d.ts",
        "default": "./dist/esm/tutorials/*.js"
      },
      "require": {
        "types": "./dist/js/tutorials/*.d.ts",
        "default": "./dist/cjs/tutorials/*.cjs"
      }
    },
    "./js/styles/*": {
      "import": {
        "types": "./dist/js/styles/*.d.ts",
        "default": "./dist/esm/styles/*.js"
      },
      "require": {
        "types": "./dist/js/styles/*.d.ts",
        "default": "./dist/cjs/styles/*.cjs"
      }
    },
    "./js/languages/*": {
      "import": {
        "types": "./dist/js/languages/*.d.ts",
        "default": "./dist/esm/languages/*.js"
      },
      "require": {
        "types": "./dist/js/languages/*.d.ts",
        "default": "./dist/cjs/languages/*.cjs"
      }
    },
    "./js/api/*": {
      "import": {
        "types": "./dist/js/api/*.d.ts",
        "default": "./dist/esm/api/*.js"
      },
      "require": {
        "types": "./dist/js/api/*.d.ts",
        "default": "./dist/cjs/api/*.cjs"
      }
    },
    "./js/messages/*": {
      "import": "./dist/esm/messages/*.json",
      "require": "./dist/cjs/messages/*.json"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node esbuild.config.cjs",
    "build:watch": "node esbuild.config.cjs --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@babel/plugin-transform-flow-strip-types": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "lodash": "^4.17.2",
    "moment": "^2.17.0",
    "react": "^19.0.0",
    "react-intl": "^7.1.14",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^24.10.1",
    "esbuild": "^0.27.0",
    "jest": "^29.7.0",
    "parse-json": "^2.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "underscore": "^1.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.test.tsx"
    ],
    "moduleNameMapper": {
      "dancedeets-common/(.*)": "<rootDir>/js/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "<rootDir>/tsconfig.json"
        }
      ]
    }
  },
  "overrides": {
    "@types/react": "^19.0.0"
  }
}
